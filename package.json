{
  "name": "unleash-client",
  "version": "1.0.0-beta.1",
  "description": "Unleash Client for Node",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "lint": "eslint .",
    "build": "tsc src/*.ts -d --strictNullChecks --sourceMap --outdir ./lib --lib ES6,ES5",
    "prepublish": "not-in-install && npm run build || in-install",
    "test": "ava test",
    "coverage": "nyc --check-coverage=true npm test",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:Unleash/unleash-client-node.git"
  },
  "keywords": [
    "unleash",
    "client",
    "feature toggle"
  ],
  "bugs": {
    "url": "https://github.com/Unleash/unleash-client-node/issues"
  },
  "homepage": "https://github.com/Unleash/unleash-client-node",
  "dependencies": {
    "request": "^2.74.0"
  },
  "engines": {
    "node": ">=6"
  },
  "files": [
    "lib",
    "examples"
  ],
  "devDependencies": {
    "@types/node": "^6.0.41",
    "@types/request": "0.0.31",
    "ava": "^0.16.0",
    "coveralls": "^2.11.2",
    "eslint": "^3.3.1",
    "eslint-config-finn": "^1.0.0-alpha.9",
    "in-publish": "^2.0.0",
    "mkdirp": "^0.5.1",
    "nock": "^9.0.0",
    "nyc": "^8.3.0",
    "typescript": "^2.0.3"
  },
  "nyc": {
    "lines": 95,
    "statements": 95,
    "functions": 95,
    "branches": 80,
    "include": [
      "lib/**.js"
    ],
    "all": true
  }
}

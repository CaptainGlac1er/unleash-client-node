{
  "name": "unleash-client",
  "version": "0.1.1",
  "description": "Unleash Client for Node",
  "main": "unleash.js",
  "scripts": {
    "test": "mocha test test/**/*.js",
    "tdd": "mocha --watch test test/**/*.js",
    "coverage": "istanbul cover _mocha --report lcovonly -- test test/**/*.js",
    "coverage-report": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:finn-no/unleash-client-node.git"
  },
  "keywords": [
    "unleash",
    "client",
    "feature toggle"
  ],
  "bugs": {
    "url": "https://github.com/finn-no/unleash-client-node/issues"
  },
  "homepage": "https://github.com/finn-no/unleash-client-node",
  "dependencies": {
    "request": "^2.53.0"
  },
  "devDependencies": {
    "chai": "^2.1.0",
    "coveralls": "^2.11.2",
    "expect.js": "^0.3.1",
    "istanbul": "^0.3.6",
    "mocha": "^2.1.0",
    "mocha-lcov-reporter": "0.0.2",
    "nock": "^0.59.1"
  }
}
